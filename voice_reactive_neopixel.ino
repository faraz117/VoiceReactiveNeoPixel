int frame = 1;
void setup() {
  Serial.begin(115200);
  FastLED.addLeds<NEOPIXEL, DATA_PIN>(leds, NUM_LEDS);
  FastLED.setBrightness(255);
  //endCredits();
}

void loop() {
  // get noise value
  unsigned long startMillis = millis(); // Start of sample window
  unsigned int peakToPeak = 0;   // peak-to-peak level
  unsigned int signalMax = 0;
  unsigned int signalMin = 1024;
    if (startMillis - previousMillis >= interval) {
    // save the last time you blinked the LED
    previousMillis = startMillis;
    frame++;
    if(frame >4)
    frame = 1;
    }
  // collect data for 50 mS
  while (millis() - startMillis < sampleWindow)
  {
    sample = analogRead(0);
    if (sample < 1024)  // toss out spurious readings
    {
      if (sample > signalMax)
      {
        signalMax = sample;  // save just the max levels
      }
      else if (sample < signalMin)
      {
        signalMin = sample;  // save just the min levels
      }
    }
  }
  peakToPeak = signalMax - signalMin;  // max - min = peak-peak amplitude
  double noise = (peakToPeak * 50.0) / 1024;  // convert to noise
  if (noise > 17) {
    mode++;
    if (mode == 1){
      red = 255;
      blue= 255;
      green = 0;
    }
   else if (mode == 2){
      red = 255;
      blue= 0;
      green =0;
    }
   else if (mode == 3){
      red = 0;
      blue= 255;
      green =255 ;
    }
    else if (mode==4){
      red = 255;
      blue= 0;
      green = 255;
    }
    else if (mode==5){
      red = 255;
      blue= 100;
      green = 50;
    }
    else if (mode==6){
      red = 255;
      blue= 0;
      green = 255;
    }
    else if (mode>6){
      mode = 1;
    }
  }
  if ( noise < minValue)
    noise = minValue;
  if (noise > maxValue)
    noise = maxValue;
  x = map(noise, minValue, maxValue - 8, 1, 8);


  // perform animation
  if (x > 0) {
    if (x > lastVal) {
      for (int i = lastVal; i < x; i++) {
        switch(frame){
          case 1:
          animationA1(i);
          break;
          case 2:
          animationA2(i);
          break;
          animationC1(i);
          break;
          case 4:
          animationA4(i);
          break;
        }
        
      }
      lastVal = x;
    }


    else {
      for (int i = lastVal; i >= x; i--) {
     switch(frame){
          case 1:
          animationA1(i);
          break;
          case 2:
          animationA2(i);
          break;
          case 3:                    
          animationC1(i);
          break;
          case 4:
          animationA4(i);
          break;

      }

      lastVal = x;
    }
  }
  }
}
